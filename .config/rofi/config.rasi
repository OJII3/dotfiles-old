configuration {
  font: "JetBranins Mono Font:style=Bold 12";
  modi: "combi,drun,run,system:~/.config/rofi/rofi-power-menu";
  combi-modi: "drun,run,window,system:~/.config/rofi/rofi-power-menu";
  sidebar-mode: true;
  kb-cancel: "Escape,Control+bracketleft";
  kb-mode-previous: "Alt+h";
  kb-mode-next: "Alt+l";
  show-icons: true;
  fallback-application-icon: "~/.config/rofi/miku-sd.png";
}

